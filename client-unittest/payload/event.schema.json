{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "rl_message": {
      "type": "object",
      "properties": {
        "rl_channel": {
          "type": "string"
        },
        "rl_context": {
          "type": "object",
          "properties": {
            "rl_app": {
              "type": "object",
              "properties": {
                "rl_build": {
                  "type": "string"
                },
                "rl_name": {
                  "type": "string"
                },
                "rl_namespace": {
                  "type": "string"
                },
                "rl_version": {
                  "type": "string"
                }
              },
              "required": [
                "rl_build",
                "rl_name",
                "rl_namespace",
                "rl_version"
              ]
            },
            "rl_library": {
              "type": "object",
              "properties": {
                "rl_name": {
                  "type": "string"
                },
                "rl_version": {
                  "type": "string"
                }
              },
              "required": [
                "rl_name",
                "rl_version"
              ]
            },
            "rl_os": {
              "type": "object",
              "properties": {
                "rl_name": {
                  "type": "string"
                },
                "rl_version": {
                  "type": "string"
                }
              },
              "required": [
                "rl_name",
                "rl_version"
              ]
            },
            "rl_screen": {
              "type": "object",
              "properties": {
                "rl_density": {
                  "type": "integer"
                },
                "rl_width": {
                  "type": "integer"
                },
                "rl_height": {
                  "type": "integer"
                }
              },
              "required": [
                "rl_density",
                "rl_width",
                "rl_height"
              ]
            },
            "rl_user_agent": {
              "type": "string"
            },
            "rl_locale": {
              "type": "string"
            },
            "rl_device": {
              "type": "object",
              "properties": {
                "rl_id": {
                  "type": "string"
                },
                "rl_manufacturer": {
                  "type": "string"
                },
                "rl_model": {
                  "type": "string"
                },
                "rl_name": {
                  "type": "string"
                }
              },
              "required": [
                "rl_id",
                "rl_manufacturer",
                "rl_model",
                "rl_name"
              ]
            },
            "rl_network": {
              "type": "object",
              "properties": {
                "rl_carrier": {
                  "type": "string"
                }
              },
              "required": [
                "rl_carrier"
              ]
            }
          },
          "required": [
            "rl_app",
            "rl_library",
            "rl_os",
            "rl_screen",
            "rl_user_agent",
            "rl_locale",
            "rl_device",
            "rl_network"
          ]
        },
        "rl_type": {
          "type": "string"
        },
        "rl_message_id": {
          "type": "string"
        },
        "rl_timestamp": {
          "type": "string"
        },
        "rl_anonymous_id": {
          "type": "string"
        },
        "rl_event": {
          "type": "string"
        },
        "rl_properties": {
          "type": "object"
        },
        "rl_integrations": {
          "type": "array",
          "items": [
            {
              "type": "string"
            }
          ]
        }
      },
      "required": [
        "rl_channel",
        "rl_context",
        "rl_type",
        "rl_message_id",
        "rl_timestamp",
        "rl_anonymous_id",
        "rl_integrations"
      ]
    }
  },
  "required": [
    "rl_message"
  ]
}
